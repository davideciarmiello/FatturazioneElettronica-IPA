{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "name": "WS10Response",
    "type": "object",
    "properties": {
        "result": {
            "type": "object",
            "properties": {
                "cod_err": {
                    "type": "number",
                    "description": "Codice di ritorno"
                },
                "desc_err": {
                    "type": "string",
                    "description": "Descrizione errore"
                },
                "num_items": {
                    "type": "number",
                    "description": "Numero di occorrenze trovate",
                    "minimum": 0
                }
            },
            "required": [
                "cod_err",
                "num_items"
            ]
        },
        "data": {
            "type": ["array", "null"],
            "items": {
                "type": "object",
                "properties": {
                    "domicilio_digitale": {
                        "type": "string",
                        "description": "Domicilio digitale"
                    },
                    "tipo": {
                        "type": "string",
                        "description": "Tipo Domicilio digitale; attualmente solo PEC"
                    },
                    "data_pubblicazione": {
                        "type": "string",
                        "description": "Data pubblicazione nuovo Domicilio digitale"
                    },
                    "cod_uni_ou": {
                        "type": "string",
                        "description": "Codice univoco ufficio"
                    },
                    "cod_ou": {
                        "type": "string",
                        "description": "Codice ufficio"
                    },
                    "des_ou": {
                        "type": "string",
                        "description": "Nome ufficio"
                    },
                    "cod_amm": {
                        "type": "string",
                        "description": "Codice Ente accreditato in IPA"
                    },
                    "des_amm": {
                        "type": "string",
                        "description": "Nome Ente "
                    },
                    "cod_aoo": {
                        "type": "string",
                        "description": "Codice AOO"
                    },
                    "des_aoo": {
                        "type": "string",
                        "description": "Nome dell'AOO"
                    }
                },
                "required": [
                    "domicilio_digitale",
                    "tipo",
                    "data_pubblicazione",
                    "cod_uni_ou",
                    "cod_ou",
                    "des_ou",
                    "cod_amm",
                    "des_amm",
                    "cod_aoo",
                    "des_aoo"
                ]
            }
        }
    },
    "required": [
        "result"
    ]
}