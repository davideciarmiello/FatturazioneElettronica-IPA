{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "name": "WS01Response",
    "type": "object",
    "properties": {
        "result": {
            "type": "object",
            "properties": {
                "cod_err": {
                    "type": "number",
                    "description": "Codice di ritorno"
                },
                "desc_err": {
                    "type": "string",
                    "description": "Descrizione errore"
                },
                "num_items": {
                    "type": "number",
                    "description": "Numero di occorrenze trovate",
                    "minimum": 0
                }
            },
            "required": [
                "cod_err",
                "num_items"
            ]
        },
        "data": {
            "type": ["array", "null"],
            "items": {
                "type": "object",
                "properties": {
                    "cod_amm": {
                        "type": "string",
                        "description": "Codice Ente accreditato in IPA"
                    },
                    "des_amm": {
                        "type": "string",
                        "description": "Denominazione Ente accreditato in IPA"
                    },
                    "ou": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "des_ou": {
                                    "type": "string",
                                    "description": "Denominazione Unita Organizzativa"
                                },
                                "stato_canale": {
                                    "type": "string",
                                    "description": "A: Canale Attivo, V: In Fase di validazione"
                                },
                                "cod_uni_ou": {
                                    "type": "string",
                                    "description": "Codice Univoco Ufficio destinatario di Fatturazione Elettroniche"
                                }
                            },
                            "required": [
                                "des_ou",
                                "stato_canale"
                            ]
                        },
                        "minItems": 1
                    }
                },
                "required": [
                    "cod_amm",
                    "des_amm",
                    "ou"
                ]
            }
        }
    },
    "required": [
        "result"
    ]
}