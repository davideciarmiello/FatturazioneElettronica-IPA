{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "name": "WS15Response",
    "type": "object",
    "properties": {
        "result": {
            "type": "object",
            "properties": {
                "cod_err": {
                    "type": "number",
                    "description": "Codice di ritorno"
                },
                "desc_err": {
                    "type": "string",
                    "description": "Descrizione errore"
                },
                "num_items": {
                    "type": "number",
                    "description": "Numero di occorrenze trovate",
                    "minimum": 0
                }
            },
            "required": [
                "cod_err",
                "num_items"
            ]
        },
        "data": {
            "type": ["array", "null"],
            "items": {
                "type": "object",
                "properties": {
                    "cod_amm": {
                        "type": "string",
                        "description": "Codice Ente accreditato in IPA"
                    },
                    "stato_canale": {
                        "type": "string",
                        "description": "Stato del canale ordini"
                    },
                    "cod_uni_ou": {
                        "type": "string",
                        "description": "Codice Univoco dell'Ufficio del nodo di smistamento ordini"
                    },
                    "cod_aoo": {
                        "type": "string",
                        "description": "Codice AOO"
                    },
                    "des_ou": {
                        "type": "string",
                        "description": "Nome dell'UO"
                    },
                    "cf": {
                        "type": "string",
                        "description": "Codice fiscale del nodo di smistamento ordini"
                    },
                    "dt_verifica_cf": {
                        "type": "string",
                        "description": "Data di validazione del cf"
                    },
                    "dat_val_canale_trasm_nso": {
                        "type": "string",
                        "description": "Data di inizio validit√† del nodo di smistamento ordini"
                    }
                },
                "required": [
                    "cod_amm",
                    "cod_uni_ou",
                    "cf"
                ]
            }
        }
    },
    "required": [
        "result"
    ]
}