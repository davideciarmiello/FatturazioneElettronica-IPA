{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "name": "WS07Response",
    "type": "object",
    "properties": {
        "result": {
            "type": "object",
            "properties": {
                "cod_err": {
                    "type": "number",
                    "description": "Codice di ritorno"
                },
                "desc_err": {
                    "type": "string",
                    "description": "Descrizione errore"
                },
                "num_items": {
                    "type": "number",
                    "description": "Numero di occorrenze trovate",
                    "minimum": 0
                }
            },
            "required": [
                "cod_err",
                "num_items"
            ]
        },
        "data": {
            "type": ["array", "null"],
            "items": {
                "type": "object",
                "properties": {
                    "tipo_email": {
                        "type": "string",
                        "description": "Tipo email:PEC|CECPAC|ALTRO"
                    },
                    "tipo_entita": {
                        "type": "string",
                        "description": "Tipo entità: AMM|AOO|UO|SERVAMM|SERVAOO|SERVOU|RESPAOO|RESPUO"
                    },
                    "cod_amm": {
                        "type": "string",
                        "description": "Codice Ente accreditato in IPA"
                    },
                    "des_amm": {
                        "type": "string",
                        "description": "Denominazione Ente accreditato in IPA"
                    },
                    "cod_entita": {
                        "type": "string",
                        "description": "Codice dell'entità"
                    }
                },
                "required": [
                    "tipo_entita",
                    "cod_amm",
                    "cod_entita"
                ]
            }
        }
    },
    "required": [
        "result"
    ]
}